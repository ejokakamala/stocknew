<div class="mx-auto w-full mt-14">
  <div class="mx-auto">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>

    <%= render @expense %>

  </div>

  <span>Batch Id</span> = <%= @expense.batch_id %> <br />
  <span>Batch no</span> = <%= @expense.batch.batch_no %>
</div>
